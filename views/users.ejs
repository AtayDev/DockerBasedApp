<!DOCTYPE html>
<html lang="en">
<head>
    <title>Users</title>
</head>
<body>
    <a href="/">Home</a>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        <% for(var i = 0; i < users.length; i++) {%>
            <tr>
                <td><%= users[i].fname %></td>
                <td><%= users[i].lname %></td>
                <td><%= users[i].email %></td>
            </tr>
        <% } %>
    </table>
</body>
</html>